<!-- Latest compiled and minified CSS -->
<link
  href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css"
  rel="stylesheet"
/>
<!-- Latest compiled JavaScript -->
<script
  src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"
  defer
></script>
<link rel="stylesheet" href="/style.css" />

<div class="container text-center card bg-primary text-light my-3">
  <h1 class="card-body">我的個人日記 APP</h1>
</div>

<% for (let i = 0; i < posts.length; i++) { %>
<div class="container text-center card bg-light my-3">
  <div class="card-body">
    <h2 class="card-title text-dark"><%=posts[i].title%></h2>
    <div class="card-text text-dark-emphasis"><%=posts[i].content%></div>
  </div>
</div>
<% } %>
